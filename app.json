{
  "expo": {
    "name": "Dashen",
    "slug": "Dashen",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/DashenSplash.png",
    "scheme": "dashin",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/DashenSplash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#003366"
        }
      ],
      "react-native-bootsplash"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
